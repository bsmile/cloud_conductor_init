{
  "watches": [
    {
      "type": "event",
      "name": "setup",
      "handler": "/opt/cloudconductor/bin/scheduler push setup"
    },
    {
      "type": "event",
      "name": "configure",
      "handler": "/opt/cloudconductor/bin/scheduler push configure"
    },
    {
      "type": "event",
      "name": "deploy",
      "handler": "/opt/cloudconductor/bin/scheduler push deploy"
    },
    {
      "type": "event",
      "name": "backup",
      "handler": "/opt/cloudconductor/bin/scheduler push backup"
    },
    {
      "type": "event",
      "name": "restore",
      "handler": "/opt/cloudconductor/bin/scheduler push restore"
    },
    {
      "type": "event",
      "name": "spec",
      "handler": "/opt/cloudconductor/bin/scheduler push spec"
    }
  ]
}
